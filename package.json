{
  "name": "ai-community",
  "version": "1.0.0",
  "description": "An AI character role-playing community platform",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "server": "nodemon server/app.js",
    "client": "cd client && npm start",
